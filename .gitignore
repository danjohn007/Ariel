# Sistema de Análisis de Precios y Programa de Obra
# .gitignore

# Archivos de configuración sensibles
.env
.env.local
.env.production

# Logs
*.log
logs/
error.log
access.log
php_errors.log

# Archivos temporales del sistema
*.tmp
*.temp
*.swp
*.swo
*~
.DS_Store
Thumbs.db

# Cache y archivos temporales de PHP
opcache/
cache/
tmp/
temp/
sessions/

# Archivos de backup de base de datos
*.sql.bak
*.sql.gz
backup/
backups/

# Archivos subidos por usuarios
uploads/
files/
documents/
!uploads/.gitkeep
!files/.gitkeep

# Node.js (si se usa para build tools)
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
package-lock.json
yarn.lock

# Composer (si se usa en el futuro)
/vendor/
composer.lock

# IDEs y editores
.vscode/
.idea/
*.sublime-workspace
*.sublime-project
*.code-workspace

# Archivos de desarrollo
.phpunit.result.cache
phpunit.xml
.phpcs.xml
.phpmd.xml

# Archivos específicos del servidor web
.htaccess.bak
web.config.bak

# Archivos de certificados SSL (si se almacenan localmente)
*.pem
*.key
*.crt
*.csr

# Archivos de configuración específicos del entorno
config/local.php
config/development.php
config/production.php

# Archivos de migración temporales
migrations/temp/
migrations/rollback/

# Reportes y exports
reports/
exports/
*.pdf
*.xlsx
*.csv
!docs/*.pdf

# Archivos de monitoreo y profiling
profiler/
monitoring/
*.prof

# Directorios de herramientas de desarrollo
tools/
dev-tools/
testing/

# Archivos específicos de Apache
.htpasswd

# Archivos específicos de Nginx
nginx.conf.local

# Variables de entorno específicas por desarrollador
.env.dev
.env.staging
.env.testing

# Archivos de configuración de SSL local
ssl/
certificates/

# Archivos de imagen temporales o de prueba
public/assets/images/temp/
public/assets/images/uploads/
!public/assets/images/.gitkeep

# Archivos CSS y JS compilados (si se usa preprocesador)
public/assets/css/compiled/
public/assets/js/compiled/
public/assets/css/*.min.css
public/assets/js/*.min.js

# Archivos de documentación generada automáticamente
docs/api/
docs/generated/

# Archivos de testing
tests/coverage/
tests/reports/
phpunit.phar

# Archivos específicos del OS
*.pid
*.seed
*.pid.lock

# Archivos de configuración de desarrollo local
docker-compose.override.yml
.docker/

# Archivos de estadísticas y métricas
stats/
metrics/
analytics/